
@{
    ViewData["Title"] = "FileUpload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model TheBakeryShopper.Models.FileTest
<h2>FileUpload</h2>
@using (Html.BeginForm("UploadFileForm", "FileHandle", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.LabelFor(m => m.FileId)
    @Html.TextBoxFor(m => m.FileId)
    <span>Select File:</span>
    
    <button type="submit">Upload File</button>


   
}


